<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.bancasegurosEmision.controller.MenuBar" xmlns="sap.m"
	xmlns:tnt="sap.tnt">
	<Page title="BANCASEGUROS" showHeader="true" showFooter="false" showNavButton="false">
		<content>
			<tnt:NavigationList width="auto" expanded="true">
				<tnt:items>
					<tnt:NavigationListItem icon="sap-icon://home" expanded="true" hasExpander="true" text="Home" select="_onNavigationListItemSelect">
						<tnt:items/>
					</tnt:NavigationListItem>
					<tnt:NavigationListItem icon="sap-icon://folder" expanded="false" hasExpander="true" text="Emisión">
