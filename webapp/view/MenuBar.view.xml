							<tnt:NavigationListItem expanded="true" hasExpander="true" text="Póliza"/>
							<tnt:NavigationListItem expanded="true" hasExpander="true" text="Agente"/>
							<tnt:NavigationListItem expanded="true" hasExpander="true" text="Certificado" select="_onNavigationListItemFirstLevelSelect"/>
						</tnt:items>
					</tnt:NavigationListItem>
					<tnt:NavigationListItem icon="sap-icon://money-bills" expanded="true" hasExpander="true" text="Cobranzas">
						<tnt:items/>
					</tnt:NavigationListItem>
					<tnt:NavigationListItem icon="sap-icon://action-settings" expanded="true" hasExpander="true" text="Admon">
						<tnt:items/>
					</tnt:NavigationListItem>
				</tnt:items>
			</tnt:NavigationList>
		</content>
		<footer>
			<Bar design="Auto">
				<contentLeft/>
				<contentMiddle/>
				<contentRight>
					<Button text="New" type="Transparent" icon="sap-icon://add" iconFirst="false" width="auto" enabled="true" visible="true"
						iconDensityAware="false"/>
				</contentRight>
			</Bar>
		</footer>
		<headerContent/>
	</Page>
</mvc:View>